    "@tailwindcss/ui": "0.6.2",
    "ajv": "^6.12.6",
    "axios": "0.21.0",
    "heroicons-react": "1.3.0",
    "immer": "^7.0.14",
    "js-yaml": "^3.14.0",
    "normalizr": "^3.6.1",
