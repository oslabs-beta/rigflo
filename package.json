{
  "name": "rigflo",
  "main": "public/electron.js",
  "browserslist": [
    "since 2017-06"
  ],
  "scripts": {
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:8080 && electron . --enable-logging\"",
    "start": "snowpack dev --polyfill-node",
    "build": "snowpack build",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "concurrently": "5.3.0",
    "electron": "10.1.4",
    "electron-is-dev": "1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "wait-on": "5.2.0"
  },
  "devDependencies": {
    "@snowpack/app-scripts-react": "^1.12.4",
    "@snowpack/plugin-dotenv": "^2.0.4",
    "@snowpack/plugin-react-refresh": "^2.3.3",
    "@snowpack/plugin-typescript": "^1.0.2",
    "@snowpack/web-test-runner-plugin": "^0.1.3",
    "@testing-library/react": "^11.0.0",
    "@types/chai": "^4.2.13",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/snowpack-env": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@web/test-runner": "^0.9.0",
    "chai": "^4.2.0",
    "electron-devtools-installer": "3.1.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-problems": "^5.0.0",
    "prettier": "^2.0.5",
    "snowpack": "^2.15.0",
    "typescript": "^4.0.0"
  }
