{
  "name": "rigflo",
  "main": "public/electron.js",
  "browserslist": [
    "since 2017-06"
  ],
  "scripts": {
    "dev:snowpack": "snowpack dev",
    "dev:ws": "nodemon ./parser/server/index.js",
    "dev:electron": "wait-on http://localhost:8080 && electron .",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind dev:snowpack",
    "prebuild": "run-s build:tailwind",
    "build": "snowpack build",
    "build:tailwind": "tailwind build src/tailwind.css -o src/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/tailwind.output.css -c 'npm run build:tailwind'",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@headlessui/react": "0.2.0",
    "@tailwindcss/ui": "0.6.2",
    "ajv": "^6.12.6",
    "chokidar-cli": "2.1.0",
    "electron": "10.1.4",
    "electron-is-dev": "1.2.0",
    "heroicons-react": "1.3.0",
    "npm-run-all": "4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-flow-renderer": "6.1.3",
    "react-json-pretty": "^2.2.0",
    "tailwindcss": "1.9.5",
    "wait-on": "5.2.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@babel/preset-react": "7.12.1",
    "@snowpack/app-scripts-react": "^1.12.4",
    "@snowpack/plugin-dotenv": "^2.0.4",
    "@snowpack/plugin-react-refresh": "^2.3.3",
    "@snowpack/web-test-runner-plugin": "^0.1.3",
    "@testing-library/react": "^11.0.0",
    "@web/test-runner": "^0.9.0",
    "chai": "^4.2.0",
    "electron-devtools-installer": "3.1.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-problems": "^5.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.0.5",
    "snowpack": "^2.15.0",
    "typescript": "^4.0.0"
  }
}
